@model WFJ.Models.AddEditPlacementsViewModel

@{
    ViewBag.Title = "AddPlacement";
    Layout = "~/Views/Shared/_Layout.cshtml";

    var FieldsGrouped = Model.FormFieldsList.GroupBy(x => x.formSectionID)
                        .Select(x => new
                        {
                            sectionId = x.Key,
                            sectionData = x.GroupBy(a => a.formSectionID).OrderBy(a => a.Key).Select(r => new
                            {
                                rowNo = r.Key,
                                rowData = r.GroupBy(b => b.rowNumber).OrderBy(b => b.Key).AsEnumerable()
                            })
                        }).ToList();
}

<main>
    <div class="document-center">
        <div class="container-fluid site-width">
            <h4 class="mb-4">View Request Customer name</h4>
            <!-- START: Breadcrumbs-->
            <div class="row">
                <div class="col-md-5 col-xl-5">
                    <div class="form-group">
                        <button type="button" class="btn btn-secondary">PDF</button>
                    </div>


                    @foreach (var section in Model.FormSections)
                    {
                        var ItemsSectionWise = FieldsGrouped.FirstOrDefault(x => x.sectionId == section.formSectionID);
                        if (ItemsSectionWise != null)
                        {
                            var SectionWise = ItemsSectionWise.sectionData;
                            foreach (var sectionData in SectionWise)
                            {
                                <div class="card mb-4">
                                    <div class="card-content">
                                        <div class="card-body">

                                            @foreach (var row in sectionData.rowData)
                                            {
                                            <div class="row">

                                                @foreach (var item in row)
                                                {
                                                    if (item.FieldTypeID != 0)
                                                    {
                                                        string colClass = item.FieldSize == null ? "col-12" : item.FieldSize.htmlCode;
                                                        string Required = item.Required == 1 ? "required" : "";
                                                        string maxLength = item.Length != null ? "maxlength=" + item.Length : "";

                                                        <div class="@colClass">
                                                            <div class="form-group">
                                                                <input type="hidden" value="@item.ID" />

                                                                <label>@item.FieldName</label>
                                                                @if (item.FieldTypeID == 1)
                                                                {
                                                                    <input type="number" class="form-control" @Required />
                                                                }
                                                                else if (item.FieldTypeID == 2)
                                                                {
                                                                    <input class="form-control jdate-picker" @Required />
                                                                }
                                                                else if (item.FieldTypeID == 3)
                                                                {
                                                                    <input type="text" class="form-control" @maxLength @Required />
                                                                }
                                                                else if (item.FieldTypeID == 4)
                                                                {
                                                                    <div>
                                                                        <label class="radio-inline"><input class="mr-1" type="radio" name="@item.ID" value="1" @Required>Yes</label>
                                                                        <label class="radio-inline"><input class="mr-1" type="radio" name="@item.ID" value="0" @Required>No</label>
                                                                    </div>
                                                                }
                                                                else if (item.FieldTypeID == 6) // text only
                                                                {
                                                                    <input type="text" pattern="[A-Za-z]" class="form-control" @maxLength @Required />
                                                                }
                                                                else if (item.FieldTypeID == 9 || item.FieldTypeID == 5) // name, telephone
                                                                {
                                                                    <input type="text" pattern="[A-Za-z0-9]" class="form-control" @maxLength @Required />
                                                                }
                                                                else if(item.FieldTypeID == 7)
                                                                {
                                                                    <select class="form-control" @Required>
                                                                        <option></option>
                                                                        @foreach(var option in item.FormSelectionLists.OrderBy(x => x.SeqNo))
                                                                        {
                                                                            <option value="@option.Code">@option.TextValue</option>
                                                                        }
                                                                    </select>
                                                                }
                                                                else if (item.FieldTypeID == 8)
                                                                {
                                                                    <input type="text" style="margin-bottom:1rem;" class="form-control" maxlength="300" placeholder="Addredd line 1" @Required />
                                                                    <input type="text" style="margin-bottom:1rem;" class="form-control" maxlength="80" placeholder="Addredd line 2" />
                                                                    <input type="text" style="margin-bottom:1rem;" class="form-control" maxlength="80" placeholder="Addredd line 3" />
                                                                    <input type="text" style="margin-bottom:1rem;" class="form-control" maxlength="50" placeholder="City" />
                                                                    <input type="text" style="margin-bottom:1rem;" class="form-control" maxlength="3" placeholder="State" />
                                                                    <input type="text" style="margin-bottom:1rem;" class="form-control" maxlength="10" placeholder="Zip code" />
                                                                    <input type="text" class="form-control" maxlength="20" placeholder="Country" />
                                                                }
                                                                else if(item.FieldTypeID == 13)
                                                                {
                                                                    <input type="checkbox"/>
                                                                }
                                                                else if (item.FieldTypeID == 19) // currency
                                                                {
                                                                    <input type="number" min="0.01" step="0.01" class="form-control" @Required />
                                                                }
                                                            </div>
                                                        </div>
                                                    }
                                                }

                                            </div>
                                            }

                                        </div>
                                    </div>
                                </div>
                            }

                        }

                    }
